{
    "productName": "KuteClip",
    "version": "0.1.0",
    "identifier": "com.kutedev.kuteclip",
    "build": {
        "beforeDevCommand": "pnpm dev",
        "devUrl": "http://localhost:1420",
        "beforeBuildCommand": "pnpm build",
        "frontendDist": "../dist"
    },
    "app": {
        "windows": [
            {
                "title": "KuteClip",
                "label": "main",
                "width": 600,
                "height": 400,
                "center": true,
                "resizable": false,
                "fileDropEnabled": false,
                "closable": false,
                "minimizable": false,
                "maximizable": false,
                "decorations": false
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    }
}